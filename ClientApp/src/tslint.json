{
    "extends": "../tslint.json",
  "rules": {
    "no-string-literal": false,
    "array-type": false,
    "deprecation": { "severity": "warning" },
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
